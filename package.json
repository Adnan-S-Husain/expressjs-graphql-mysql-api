{
  "name": "poc-backend-graphql-express",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "pm2 start ./dist/server.js",
    "stop": "pm2 stop all",
    "dockerup": "docker-compose -f docker.yaml up -d",
    "dockerdown": "docker-compose -f docker.yaml down",
    "dev": "yarn generate && nodemon ./server.ts",
    "build": "yarn generate && tsc -p .",
    "deploy": "yarn install && yarn build",
    "generate": "graphql-codegen --config codegen.config.ts"
  },
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "jest": "^29.4.3",
    "jsonwebtoken": "^9.0.0",
    "log4js": "^6.7.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.0.1",
    "pm2": "^5.2.2",
    "sequelize": "^6.28.0",
    "tsc": "^2.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.1",
    "@graphql-codegen/typescript": "3.0.1",
    "@graphql-codegen/typescript-resolvers": "3.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
